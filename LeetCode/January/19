if(head==null || k==0){
            return head;
        }
        
        Node current = head;
        int length=1;
        while(current.next!=null){
            current=current.next;
            length++;
        }
        current.next = head; 
        k = k%length;
        
        for(int i=0;i<k;i++){
            current = current.next;
        }
        
        head = current.next;
        current.next=null;
        
        return head;
